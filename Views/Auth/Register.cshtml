@model FileHosting.Models.ViewModels.RegisterViewModel

<h2>Registrieren</h2>
<form method="post">
    <input asp-for="Username" placeholder="Benutername"/>
    <input asp-for="Email" placeholder="E-Mail"/>
    <input asp-for="Password" type="password" placeholder="Passwort"/>
    <button type="submit">Registieren</button>
</form>